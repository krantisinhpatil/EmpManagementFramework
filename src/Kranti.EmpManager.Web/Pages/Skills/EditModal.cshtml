@page
@using Kranti.EmpManager.Localization
@using Kranti.EmpManager.Web.Pages.Skill;
@using Kranti.EmpManager.Web.Pages.Skills
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model EditModalModel
@inject IStringLocalizer<EmpManagerResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="Skill" asp-page="/Skills/EditModal">
    <abp-modal>
        <abp-modal-header title="@L["Update"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-input asp-for="Id" />
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
